{
    "name": "fsuuaas/laravel-plupload",
    "description": "Plupload package for Laravel 12",
    "keywords": [
        "laravel",
        "plupload"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Linh Tran",
            "email": "jenky.w0w@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^12.0",
        "illuminate/support": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Fsuuaas\\LaravelPlupload\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Fsuuaas\\LaravelPlupload\\PluploadServiceProvider"
            ],
            "aliases": {
                "Plupload": "Fsuuaas\\LaravelPlupload\\Facades\\Plupload"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
